{
  "manifest_version": 3,
  "name": "Minha Extensão",
  "version": "1.0",
  "description": "Descrição da extensão.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
