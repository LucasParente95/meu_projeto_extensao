{
    "manifest_version": 3,
    "name": "Assistente de Resumo de Vídeos",
    "version": "1.0",
    "description": "Uma extensão para capturar o áudio de vídeos e gerar resumos automáticos.",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "imgs/icon16.png",
            "48": "imgs/icon48.png",
            "128": "imgs/icon128.png"
        }
}
